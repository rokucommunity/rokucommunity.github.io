{
    "version": "0.2.0",
    "configurations": [
        {
            "command": "./node_modules/.bin/astro dev",
            "name": "Development server",
            "request": "launch",
            "type": "node-terminal"
        },
        {
			"name": "Debug create-whats-new",
			"type": "node",
			"request": "launch",
			"args": [
                "--loader",
                "ts-node/esm/transpile-only",
				"./scripts/create-whats-new.ts",
                "--force",
                "--year",
                "2022",
                "--month",
                "november",
                "--noclear"
			],
			"cwd": "${workspaceRoot}",
			"internalConsoleOptions": "openOnSessionStart"
		},
    ]
}